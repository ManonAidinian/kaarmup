<div class="form-container">

  <h2>Edit my charity:</h2>
  <%= simple_form_for(@charity) do |f| %>
      <%= f.input :name%>
      <%= f.input :description%>
      <%= f.input :location%>
      <%= f.input :photos, as: :file, input_html: { multiple: true} %>
      <%= f.button :submit, "Edit charity"%>
    <%end%>

</div>
