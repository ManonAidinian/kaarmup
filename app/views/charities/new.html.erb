<div class="container">

  <h2>Register a new charity:</h2>
  <%= simple_form_for(@charity) do |f| %>
      <%= f.input :name%>
      <%= f.input :description%>
      <%= f.input :location%>
      <%= f.input :photo, as: :file %>
      <%= f.button :submit, "Register my charity"%>
    <%end%>

</div>
