<div class="form-container">

  <h2>Register your charity details and get on board!</h2>
  <p>You can always edit your infomations later</p>

  <%= simple_form_for(@charity) do |f| %>
      <%= f.input :name%>
      <%= f.input :description%>
      <%= f.input :location%>
      <%= f.input :photos, as: :file %>
      <%= f.button :submit, "Register my charity"%>
    <%end%>

</div>
