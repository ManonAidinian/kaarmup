<div class="container">

  <h2>Register a new company:</h2>
  <%= simple_form_for(@company) do |f| %>
      <%= f.input :name%>
      <%= f.input :description%>
      <%= f.input :location%>
      <%= f.input :photo, as: :file %>
      <%= f.button :submit, "Register my company"%>
    <%end%>

</div>
