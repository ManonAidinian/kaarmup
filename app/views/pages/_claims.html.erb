<% if @claims.empty? %>
  <p>You have no ongoing claims</p>
<%else %>
  <% @claims.each do |claim| %>
    <% if claim.status == "On process" %>
      <p> <%= link_to "Claim ##{claim.id} - #{claim.need.title} > status: #{claim.status}", claim_path(claim.id) %> </p>
    <% end %>
  <% end %>
<% end %>

<%= link_to "See claims history", history_path%>
